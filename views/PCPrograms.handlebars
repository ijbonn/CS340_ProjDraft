
<h1>Add PC Programs</h1>

<form method="post" action="">
    <ul class="formalList">
        <li><label for="pcProgDesc">PC Program Description: </label>
            <input type="text" name="pcProgDesc" id="pcProgDesc" placeholder="Description"></li>
        <input id="form" type="submit" value="Submit">
    </ul>
</form>


<div>
    <h1>Current Data:</h1>
    <table id="PCProgramsTable" class="formalTable">
        <thead>
            <tr>
                <th>PC Program ID</th>
                <th>Program Description</th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            {{#results}}
            <tr>
                <td>{{this.pcProgID}}</td>
                <td>{{this.pcProgDesc}}</td>
                <td><input id="D{{this.id}}" type="Submit" value="Delete"></td>
                <td><input id="u{{this.id}}" type="Submit" value="Update"></td>
            </tr>
            {{/results}}
        </tbody>
    </table>
</div>